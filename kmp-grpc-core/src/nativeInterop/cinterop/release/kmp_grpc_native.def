headers = include/rpcnative.h
compilerOpts = -I../kmp-grpc-native/
package = io.github.timortel.kmpgrpc.native
staticLibraries = libkmp_grpc_native.a

# apple
libraryPaths.ios_arm64 = ../kmp-grpc-native/target/aarch64-apple-ios/release
libraryPaths.ios_simulator_arm64 = ../kmp-grpc-native/target/aarch64-apple-ios-sim/release
libraryPaths.macos_arm64 = ../kmp-grpc-native/target/aarch64-apple-darwin/release
libraryPaths.macos_x64 = ../kmp-grpc-native/target/x86_64-apple-darwin/release
libraryPaths.ios_x64 = ../kmp-grpc-native/target/x86_64-apple-ios/release

# linux
libraryPaths.linux_x64 = ../kmp-grpc-native/target/x86_64-unknown-linux-gnu/release
libraryPaths.linux_arm64 = ../kmp-grpc-native/target/aarch64-unknown-linux-gnu/release

# android
libraryPaths.android_arm32 = ../kmp-grpc-native/target/armv7-linux-androideabi/release
libraryPaths.android_arm64 = ../kmp-grpc-native/target/aarch64-linux-android/release
libraryPaths.android_x86 = ../kmp-grpc-native/target/i686-linux-android/release
libraryPaths.android_x64 = ../kmp-grpc-native/target/x86_64-linux-android/release

# windows
libraryPaths.mingw_x64 = ../kmp-grpc-native/target/x86_64-pc-windows-gnu/release
